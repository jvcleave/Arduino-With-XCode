// Basic settings for an Arduino Board

ARDUINO_APP_PATH = /Applications/Arduino.app
BOARDS_TXT_PATH = /Contents/Resources/Java/hardware/arduino/boards.txt
ARDUINO_PATH = /Contents/Resources/Java/hardware/arduino/cores/arduino
ARDUINO_LIBRARIES_PATH = /Contents/Resources/Java/libraries
ARDUINO_LIBRARIES = Wire Wire/utility EEPROM Ethernet Ethernet/utility  SPI Firmata LiquidCrystal Matrix Sprite SD SD/utility Servo SoftwareSerial Stepper 
BOARDNAME = atmega328 atmega168 atmega8 bt bt328 diecimila fio lilypad lilypad328 mega mega2560 mini pro5v pro5v328 uno      

BOARDNAME = atmega328
PORT = /dev/tty.usbserial-A800evLX
//PORT = /dev/tty.usbserial* or /dev/tty.usbmodem*
